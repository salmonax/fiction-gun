<% provide :title, "Home" %>


<h1>Fiction Gun</h1>

<h2>A collaborative online writing space</h2>

<table>
  <tr>
    <td>Title</td>
    <td>Text</td>
    <td>Contributor</td>
    <td>Genre</td>
  </tr>
  <% @stories.each do |story| %>
    <tr>
      <td><%= story.title %></td>
      <td><%= story.text %></td>
      <td><%= story.contributor_id %></td>
      <td><%= story.genre_id %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to 'Add story', new_story_path %></p>