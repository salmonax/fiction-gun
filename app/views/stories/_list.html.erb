<table class="table">
  <tr>
    <td>Title</td>
    <td>Contributor</td>
    <td>Genre</td>
  </tr>
  <% stories.each do |story| %>
    <tr>
      <td><%= link_to story.title, story_path(story.id), :method => :get %></td>
      <td><%= story.users.first.pen_name %></td>
      <td><%= story.try(:genre).try(:name) %></td>
    </tr>
  <% end %>
</table>